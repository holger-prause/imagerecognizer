<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.BorderPane?>
<BorderPane prefHeight="700.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="playground.ui.ObjectsTabController">
   <top>
      <TabPane tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
        <tabs>
          <Tab text="Detect Faces">
            <content>
                <fx:include fx:id="facesTab" source="FacesTab.fxml" />
            </content>
          </Tab>
          <Tab text="Detect Objects">
            <content>
                <fx:include fx:id="objectsTab" source="ObjectsTab.fxml" />
            </content>
          </Tab>
        </tabs>
      </TabPane>
   </top>
</BorderPane>
